{
  "name": "get-json-config",
  "version": "0.2.1",
  "description": "Configutaion getter stored in json-files",
  "repository": "https://github.com/unmyke/get-json-config.git",
  "author": "unmyke <unmyke@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  },
  "scripts": {
    "start": "yarn run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "del-cli dist",
    "commitlint": "commitlint",
    "lint": "eslint --cache src",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "prebuild": "yarn run clean",
    "prepublish": "yarn run build",
    "release": "standard-version",
    "release:ci": "conventional-github-releaser -p angular",
    "release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
    "security": "nsp check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "ci:lint": "yarn run lint && yarn run security",
    "ci:test": "yarn run test -- --runInBand",
    "ci:coverage": "yarn run test:coverage -- --runInBand"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "conventional-github-releaser": "^3.1.2",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "del": "^4.1.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^8.1.5",
    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
